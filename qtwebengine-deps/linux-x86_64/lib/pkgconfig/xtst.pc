# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Xtst
Description: The Xtst Library
Version: 1.2.2
Requires: recordproto
Requires.private: x11 xext xextproto xi
Cflags: -I${includedir}
Libs: -L${libdir} -lXtst

# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Xdmcp
Description: X Display Manager Control Protocol library
Version: 1.1.2
Requires: xproto
Cflags: -I${includedir}
Libs: -L${libdir} -lXdmcp

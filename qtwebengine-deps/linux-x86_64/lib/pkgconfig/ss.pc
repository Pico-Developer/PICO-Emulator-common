# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ss
Description: Subsystem command parsing library
Version: 1.42.13
Requires.private: com_err
Cflags: -I${includedir}/ss -I${includedir}
Libs: -L${libdir} -lss
Libs.private: -ldl

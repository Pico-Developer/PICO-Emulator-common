# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Xrender
Description: X Render Library
Version: 0.9.6
Requires: xproto renderproto >= 0.9 x11
Requires.private: x11
Cflags: -I${includedir}
Libs: -L${libdir} -lXrender

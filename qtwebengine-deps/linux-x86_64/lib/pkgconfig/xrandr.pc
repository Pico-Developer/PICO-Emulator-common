# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Xrandr
Description: X RandR Library
Version: 1.4.2
Requires: xproto randrproto >= 1.4
Requires.private: x11 xext xrender
Cflags: -I${includedir}
Libs: -L${libdir} -lXrandr

# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Xdamage
Description: X Damage  Library
Version: 1.1.4
Requires: xproto damageproto >= 1.1 xfixes
Requires.private: x11 xfixes
Cflags: -I${includedir}
Libs: -L${libdir} -lXdamage

# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: expat
Version: 2.1.0
Description: expat XML parser
URL: http://www.libexpat.org
Libs: -L${libdir} -lexpat
Cflags: -I${includedir}

# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ext2fs
Description: Ext2fs library
Version: 1.42.13
Requires.private: com_err
Cflags: -I${includedir}/ext2fs -I${includedir}
Libs: -L${libdir} -lext2fs

# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: libdrm
Description: Userspace interface to kernel DRM services
Version: 2.4.0
Libs: -L${libdir} -ldrm
Cflags: -I${includedir} -I${includedir}/libdrm

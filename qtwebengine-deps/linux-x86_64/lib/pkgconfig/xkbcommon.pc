# Call pkg-config with --define-variable=qtwebengine_deps_dir=<path_to_dir> to set the prefix.
prefix=${qtwebengine_deps_dir}
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: xkbcommon
Description: XKB API common to servers and clients
Version: 0.8.2
Cflags: -I${includedir}
Libs: -L${libdir} -lxkbcommon
